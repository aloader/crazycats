<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Sample Servlet Example</display-name>

	<servlet>
		<servlet-name>SumTwoNumbersServlet</servlet-name>
		<display-name>SumTwoNumbersServlet</display-name>
		<description>Return sum of two numbers</description>
		<servlet-class>com.test.servlet.SumTwoNumbersServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SumTwoNumbersServlet</servlet-name>
		<url-pattern>/SumTwoNumbersServlet</url-pattern>
	</servlet-mapping>

</web-app>
